<div class="tab-pane fade" id="notes" role="tabpanel">

    <div class="row justify-content-between">
        <div class="col-12 col-md-4 mb-1">
            <%- include(`${partials}/sidebar/button`, {id: 'notes', text: 'Filter'}) %>
        </div>
    </div>
    
	<table class="table table-sm table-hover">
        <thead class="thead-dark" id="tbl_notes_head">
            <th class='w-30' data-column='["createdAt"]' data-dir="DESC">Date</th>
            <th class='w-70' data-column='["note"]'>Note</th>
            <th><i class="fas fa-search"></i></th>
        </thead>
        <tbody id="tbl_notes"></tbody>
    </table>
    <%- include(`${partials}/limit`, {table: 'notes'}) %>
    
</div>