<div id='mdl_<%= id %>' class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <form class="modal-content" id='form_<%= id %>'>
            <%- include(`${partials}/modals/header`, {title: title}) %>
            <div class="modal-body">
                <%- include(`${partials}/canteen/count_line`, {text: '1p',  name: 'c0001', step: '0.01'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '2p',  name: 'c0002', step: '0.02'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '5p',  name: 'c0005', step: '0.05'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '10p', name: 'c0010', step: '0.10'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '20p', name: 'c0020', step: '0.20'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '50p', name: 'c0050', step: '0.50'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£1',  name: 'c0100', step: '1'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£2',  name: 'c0200', step: '2'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£5',  name: 'c0500', step: '5'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£10', name: 'c1000', step: '10'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£20', name: 'c2000', step: '20'}) %>
                <%- include(`${partials}/canteen/count_line`, {text: '£50', name: 'c5000', step: '50'}) %>
                <%- include(`${partials}/inputs/display`, {id: 'balance_total', title: 'Total', reload_id: 'balance_reset'}) %>
                <script src="/js/canteen/count_form.js"></script>
            </div>
            <%- include(`${partials}/modals/footer`, {submit: {text: submit_text}}) %>
        </form>
    </div>
</div>