            <table class="table table-sm table-hover">
                <thead class="thead-dark" id="tbl_genders_head">
                    <th class='w-100' data-column='["gender"]' data-dir="ASC">Gender</th>
                    <th><i class='fas fa-search'></i></th>
                </thead>
                <tbody id='tbl_genders'></tbody>
            </table>
        </div>
<% } %>
